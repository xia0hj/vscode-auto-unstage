{
  "scripts": {
    "build": "node esbuild.mjs",
    "eslint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "package": "npm run build && vsce package --no-dependencies"
  },
  "main": "./dist/extension.cjs",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "auto-unstage.addSelectedRows",
        "title": "Add selected rows which will auto unstaged"
      },
      {
        "command": "auto-unstage.removeSelectedRows",
        "title": "Remove selected rows which will auto unstaged"
      },
      {
        "command": "auto-unstage.navigateByItem",
        "title": "goto"
      },
      {
        "command": "auto-unstage.removeTreeItem",
        "title": "remove"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "auto-unstage.explorer",
          "name": "Auto Unstage: Explorer"
        }
      ]
    },
    "configuration": {
      "title": "Auto Unstage",
      "properties": {
        "auto-unstage.opacity": {
          "type": "number",
          "default": 0.3,
          "description": "opacity of unstage row."
        }
      }
    },
    "menus": {
      "view/item/context": [
        {
          "command": "auto-unstage.removeTreeItem",
          "when": "viewItem == auto-unstage.removeTreeItem"
        }
      ]
    }
  },
  "extensionDependencies": [
    "vscode.git"
  ],
  "engines": {
    "vscode": "^1.93.0"
  },
  "publisher": "xia0hj",
  "name": "auto-unstage",
  "displayName": "Auto Unstage",
  "version": "0.0.0",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@stylistic/eslint-plugin": "^2.7.2",
    "@types/eslint__js": "^8.42.3",
    "@types/vscode": "^1.93.0",
    "@vscode/vsce": "^3.1.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^2.0.5"
  },
  "private": true,
  "license": "MIT"
}